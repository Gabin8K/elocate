{
  "expo": {
    "name": "Elocate",
    "slug": "elocate",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/app/icon.png",
    "scheme": "elocate",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "backgroundColor": "#1DA445"
    },
    "ios": {
      "bundleIdentifier": "com.elocate",
      "supportsTablet": true
    },
    "android": {
      "package": "com.elocate",
      "adaptiveIcon": {
        "backgroundColor": "#1DA445"
      },
      "googleServicesFile": "./google-services.json"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
