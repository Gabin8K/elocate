{
  "expo": {
    "name": "Elocate CMR",
    "slug": "elocate-cmr",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/app/icon.png",
    "scheme": "elocate",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/app/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#1DA445"
    },
    "androidStatusBar": {
      "backgroundColor": "#1DA445"
    },
    "androidNavigationBar": {
      "backgroundColor": "#1DA445"
    },
    "ios": {
      "bundleIdentifier": "com.dnfg.elocate",
      "googleServicesFile": "./GoogleService-Info.plist",
      "supportsTablet": true,
      "infoPlist": {
        "NSLocationAlwaysUsageDescription": "Allow Elocate to use your location.",
        "NSLocationWhenInUseUsageDescription": "Allow Elocate to use your location.",
        "NSPhotoLibraryUsageDescription": "The app accesses your photos to let you share them with your friends.",
        "UIApplicationExitsOnSuspend": true,
        "LSApplicationQueriesSchemes": [
          "elocate"
        ]
      }
    },
    "android": {
      "package": "com.dnfg.elocate",
      "adaptiveIcon": {
        "backgroundColor": "#1DA445"
      },
      "permissions": [
        "android.permission.VIBRATE",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.ACCESS_BACKGROUND_LOCATION"
      ],
      "googleServicesFile": "./google-services.json",
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "elocate"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        },
        {
          "action": "SEND",
          "data": {
            "mimeType": "text/plain",
            "scheme": "elocate"
          },
          "category": [
            "DEFAULT"
          ]
        }
      ]
    },
    "plugins": [
      "@react-native-google-signin/google-signin",
      "@react-native-firebase/app",
      "@react-native-firebase/auth",
      "expo-router",
      "expo-localization",
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static"
          }
        }
      ],
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow $(PRODUCT_NAME) to use your location."
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "b5dfbff6-3d7a-4b35-89c2-88b4524079e9"
      }
    }
  }
}