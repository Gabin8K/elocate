{
  "expo": {
    "name": "Elocate CMR",
    "slug": "elocate-cmr",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/app/icon.png",
    "scheme": "elocate",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/app/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#1DA445"
    },
    "androidStatusBar": {
      "backgroundColor": "#1DA445"
    },
    "androidNavigationBar": {
      "backgroundColor": "#1DA445"
    },
    "ios": {
      "bundleIdentifier": "com.dnfg.elocate",
      "googleServicesFile": "./GoogleService-Info.plist",
      "supportsTablet": true
    },
    "android": {
      "package": "com.dnfg.elocate",
      "adaptiveIcon": {
        "backgroundColor": "#1DA445"
      },
      "permissions": [
        "android.permission.VIBRATE"
      ],
      "googleServicesFile": "./google-services.json"
    },
    "plugins": [
      "@react-native-google-signin/google-signin",
      "@react-native-firebase/app",
      "@react-native-firebase/auth",
      "expo-router",
      "expo-localization",
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static"
          }
        }
      ],
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow Elocate to use your location."
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "b5dfbff6-3d7a-4b35-89c2-88b4524079e9"
      }
    }
  }
}